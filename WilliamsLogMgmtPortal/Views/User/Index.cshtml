            @Html.DisplayFor(modelItem => item.ActiveDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InactiveDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserId }) |
